@model IList<Simir.Application.ViewModels.LimpezasCadastradosVM>

        <table id="example" class="table table-hover" cellspacing="0">
            <thead>
                <tr>
                    <th style="text-align: center;">@Html.DisplayNameFor(model => model.FirstOrDefault().nmProgramacao)</th>
                    <th style="text-align: center;">@Html.DisplayNameFor(model => model.FirstOrDefault().nmBairro)</th>
                    <th style="text-align: center;">@Html.DisplayNameFor(model => model.FirstOrDefault().nmLogradouro)</th>
                    <th style="text-align: center;">@Html.DisplayNameFor(model => model.FirstOrDefault().nmRegiaoAdmin)</th>
                    <th style="text-align: center;">@Html.DisplayNameFor(model => model.FirstOrDefault().bFeiraLivre)</th>
                    <th></th>
                </tr>
            </thead>
            <tbody>
                @for (int i = 0; i < Model.Count; i++)
                {
                    <tr>
                        <td align='center'>
                            @Html.DisplayFor(item => item[i].nmProgramacao)
                        </td>
                        <td align='center'>
                            @Html.DisplayFor(item => item[i].nmBairro)
                        </td>
                        <td align='center'>
                            @Html.DisplayFor(item => item[i].nmLogradouro)
                        </td>
                        <td align='center'>
                            @Html.DisplayFor(item => item[i].nmRegiaoAdmin)
                        </td>
                        <td align='center'>
                            @Html.DisplayFor(item => item[i].bFeiraLivre)
                        </td>
                        <td>
                            <a href="@Url.Action("Index", "ServicosOrdinarios", new { area = "Limpeza", id = Model[i].LimpezaID })">
                                <div class="btn btn-primary btn-xs"><i class="fa fa-edit "></i></div>
                            </a>

                        </td>
                    </tr>
                }
            </tbody>
        </table>

<script>

    $(document).ready(function () {
        $('#example').dataTable({
            "searching": false,
            "lengthChange": false
        });
    });
</script>