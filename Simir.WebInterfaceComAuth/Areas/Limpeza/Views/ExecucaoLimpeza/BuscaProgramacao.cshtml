@model IList<Simir.Application.ViewModels.ConsultaProgramacaoVM>

    <table id="example" class="table table-hover" cellspacing="0">
        <thead>
            <tr>
                <th>@Html.DisplayNameFor(model => model.FirstOrDefault().nmProgramacao)</th>
                <th>@Html.DisplayNameFor(model => model.FirstOrDefault().dtFim)</th>
                <th>@Html.DisplayNameFor(model => model.FirstOrDefault().ServicoOrdinario)</th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @for (int i = 0; i < Model.Count; i++)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(item => item[i].nmProgramacao)
                    </td>
                    <td>
                        @Html.DisplayFor(item => item[i].dtFim)
                    </td>
                    <td>
                        @Html.DisplayFor(item => item[i].ServicoOrdinario)
                    </td>
                    <td>
                        <a href="@Url.Action("Index", "ExecucaoLimpeza", new { area = "Limpeza", id = Model[i].LimpezaID })">
                            <div class="btn btn-primary btn-xs"><i class="fa fa-edit "></i></div>
                        </a>
                    </td>
                </tr>
            }
        </tbody>
    </table>

    <script>
        $(document).ready(function () {
            $('#example').dataTable({
                "searching": false,
                "lengthChange": false
                });
        });
    </script>

